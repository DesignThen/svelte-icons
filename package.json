{
	"name": "@inqling/svelte-icons",
	"version": "2.4.0",
	"license": "MIT",
	"author": {
		"name": "Ollie Taylor <OllieJT>",
		"email": "ollie@inqling.studio",
		"url": "https://inqling.studio/"
	},
	"homepage": "https://github.com/inqling/svelte-icons#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/inqling/svelte-icons.git"
	},
	"type": "module",
	"scripts": {
		"build": "svelte-check --tsconfig ./tsconfig.json && vite build",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"clone": "yarn run clone:simple-icons && yarn run clone:heroicons",
		"clone:heroicons": "./scripts/clone-heroicons.sh > import.clone:heroicons.log",
		"clone:simple-icons": "./scripts/clone-simple-icons.sh > import.clone:simple-icons.log",
		"dev": "vite dev",
		"icon:import": "node ./scripts/import.cjs > ./import.log  && yarn lint",
		"icon:update": "yarn run clone && yarn run icon:import",
		"lint": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"package": "svelte-package",
		"postinstall": "svelte-kit sync",
		"preview": "vite preview",
		"ts": "tsc --pretty --noImplicitAny --noEmit"
	},
	"lint-staged": {
		"*.{ts,svelte,cjs,json,css,html}": "prettier  --write"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.39",
		"@sveltejs/kit": "^1.0.0-next.428",
		"@sveltejs/package": "^1.0.0-next.1",
		"@types/node": "^18.7.8",
		"cheerio": "^1.0.0-rc.12",
		"degit": "^2.8.4",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.14",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.9"
	},
	"peerDependencies": {
		"svelte": ">= 3.0.0 < 4"
	}
}

{
	"name": "@inqling/svelte-icons",
	"version": "0.2.2",
	"author": {
		"name": "Ollie Taylor <OllieJT>",
		"email": "ollie@inqling.studio",
		"url": "https://inqling.studio/"
	},
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-check --tsconfig ./tsconfig.json && svelte-kit build",
		"package": "svelte-kit package",
		"publish": "svelte-kit package && npm publish ./package --access=public",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"ts": "tsc --pretty --noImplicitAny --noEmit"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.14",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	},
	"homepage": "https://github.com/Inqling/svelte-icons#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/Inqling/svelte-icons.git"
	},
	"type": "module"
}

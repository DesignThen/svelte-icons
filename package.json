{
	"name": "@inqling/svelte-icons",
	"version": "2.4.0",
	"license": "MIT",
	"author": {
		"name": "Ollie Taylor <OllieJT>",
		"email": "ollie@inqling.studio",
		"url": "https://inqling.studio/"
	},
	"homepage": "https://github.com/inqling/svelte-icons#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/inqling/svelte-icons.git"
	},
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. .",
		"format": "pnpm lint --write",
		"release": "pnpm build && changeset publish",
		"ts": "tsc --pretty --noImplicitAny --noEmit",
		"clone": "pnpm run clone:simple-icons && pnpm run clone:heroicons",
		"clone:heroicons": "./scripts/clone-heroicons.sh > import.clone:heroicons.log",
		"clone:simple-icons": "./scripts/clone-simple-icons.sh > import.clone:simple-icons.log",
		"icon:import": "node ./scripts/import.cjs > ./import.log  && yarn lint",
		"icon:update": "pnpm run clone && pnpm run icon:import"
	},
	"lint-staged": {
		"*.{ts,svelte,cjs,json,css,html}": "prettier  --write"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.0",
		"@sveltejs/adapter-static": "^2.0.1",
		"@sveltejs/kit": "^1.8.5",
		"@sveltejs/package": "^2.0.2",
		"@types/node": "^18.14.1",
		"cheerio": "^1.0.0-rc.12",
		"degit": "^2.8.4",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.2",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier": "^2.8.4",
		"svelte-check": "^3.0.4",
		"svelte-preprocess": "^5.0.1",
		"svelte": "^3.55.1",
		"svelte2tsx": "^0.6.2",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5",
		"vite": "^4.1.4"
	},
	"peerDependencies": {
		"svelte": "^3"
	},
	"engines": {
		"pnpm": "^7.0.0"
	}
}

{
	"name": "@inqling/svelte-icons",
	"version": "0.2.2",
	"license": "MIT",
	"author": {
		"name": "Ollie Taylor <OllieJT>",
		"email": "ollie@inqling.studio",
		"url": "https://inqling.studio/"
	},
	"homepage": "https://github.com/inqling/svelte-icons#readme",
	"repository": {
		"type": "git",
		"url": "https://github.com/inqling/svelte-icons.git"
	},
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-check --tsconfig ./tsconfig.json && vite build",
		"package": "svelte-kit package",
		"publish": "svelte-kit package && npm publish ./package --access=public",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"ts": "tsc --pretty --noImplicitAny --noEmit",
		"postinstall": "svelte-kit sync"
	},
	"lint-staged": {
		"*.{ts,svelte,cjs,json,css,html}": "prettier  --write"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0-next.65",
		"@sveltejs/kit": "^1.0.0-next.428",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.1",
		"svelte-preprocess": "^4.10.7",
		"svelte2tsx": "^0.5.14",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4",
		"vite": "^3.0.9"
	}
}
